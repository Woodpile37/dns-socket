  },
  "dependencies": {
    "@leichtgewicht/dns-packet": "^6.0.3"
  },
  "devDependencies": {
    "@leichtgewicht/esm2umd": "^0.1.1",
    "c8": "^7.11.2",
    "standard": "^17.0.0",
    "tape": "^5.5.3"
